@model IEnumerable<Factory.DAL.Models.Home.Partner>

@{
    ViewData["Title"] = "Home";
} 

<!-- banner-one -->
<section class="banner__one p_relative d-flex justify-content-center align-items-center vh-100">
    <div class="image-layer-01" style="background-image:url(assets/images/shape/shape-01.png)"></div>
    <div class="image-layer-02" style="background-image:url(assets/images/shape/shape-02.png)"></div>
    @await Html.PartialAsync("ErrorSummaryPartial")

    <div class="banner__one__content">
        <div class="banner__one__left">
            <div class="sub__title">
                <span>Welcome to Your Future Home</span>
            </div>
            <div class="main__title">
                <h1>Find the Perfect Place to <br> <span>Live and Grow with Your Family.</span></h1>
            </div>
        </div>
    </div>


