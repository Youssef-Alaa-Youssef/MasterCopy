<!-- work-section -->
<section class="work__section" style="margin-top:50px;margin-bottom:50px;">
    <div class="container">
        <div class="sec-title text-center mb-5">
            <h2 class="heading-properties"><span>Our Real Estate Partners</span></h2>
            <p class="description">We proudly collaborate with top real estate professionals to bring you the best services and opportunities in the market.</p>
        </div>     
        <div class="work__content">
            <div class="row">
                @foreach (var partner in Model)
                {
                    <div class="col-lg-3 col-md-6 col-sm-12">
                        <div class="inner__box">
                        <div class="work__content">
                            <div class="work__imgae">
                                <figure>
                                    <img src="@partner.LogoUrl" alt="">
                                </figure>
                            </div>
                            <div class="work__text">
                                    <h4>@partner.Name</h4>
                            </div>
                        </div>
                    </div>
                </div>
                }
            </div>
        </div>
    </div>
</section>
